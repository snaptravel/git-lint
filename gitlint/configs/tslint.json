{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": [true, "never"],
        "quotemark": {
            "options": ["single", "avoid-escape"]
        },
        "object-literal-sort-keys": false,
        "no-console": false,
        "interface-name": [true, "never-prefix"],
        "no-var-requires": false
    },
    "rulesDirectory": []
}
